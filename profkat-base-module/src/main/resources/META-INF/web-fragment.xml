<?xml version="1.0" encoding="UTF-8"?>
<web-fragment metadata-complete="true" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd">  
  <!-- @metadata-complete -> do not process annotations -->
  <name>profkatbase</name>
  <display-name>Professorenkatalog Base</display-name>
  <description></description>

  <ordering>
    <before>
      <name>jspdocportal</name>
    </before>
  </ordering>

  <servlet id="ProfKatBeaconDataServlet">
    <servlet-name>ProfKatBeaconDataServlet</servlet-name>
    <servlet-class>org.mycore.profkat.pndbeacon.ProfKatBeaconDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProfKatBeaconDataServlet</servlet-name>
    <url-pattern>/profkat_beacon_data</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>MCRProfkatGNDServlet</servlet-name>
    <servlet-class>org.mycore.profkat.servlets.MCRProfkatGNDServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MCRProfkatGNDServlet</servlet-name>
    <url-pattern>/gnd/*</url-pattern>
  </servlet-mapping>
  <!-- Legacy Mapping -->
  <servlet-mapping>
    <servlet-name>MCRProfkatGNDServlet</servlet-name>
    <url-pattern>/pnd/*</url-pattern>
  </servlet-mapping>
</web-fragment>